                        mnemonicParsing="false" onAction="#saveButtonPressed" text="Save"/>
                <Label layoutX="236.0" layoutY="191.0" text="Version:"/>
                <Label fx:id="versionLabel" layoutX="286.0" layoutY="191.0"/>
                <TextArea fx:id="textArea" layoutX="156.0" layoutY="236.0" prefHeight="150.0" prefWidth="288.0"/>
