            fx:controller="com.missionlink.ui.Controller">
    <center>
        <Pane maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="458.0" prefWidth="601.0">
            <children>
                <Button fx:id="inputButton" layoutX="356.0" layoutY="72.0" mnemonicParsing="false"
                        onAction="#handleInputButtonClick" text="Input"/>
